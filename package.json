{
  "name": "@variancehq/variance-js",
  "version": "0.0.2",
  "description": "Integrate Variance analytics into any web application",
  "main": "dist/index.js",
  "type": "module",
  "repository": "https://github.com/variancehq/variance-js.git",
  "license": "MIT",
  "files": [
    "dist/",
    "src/",
    "snippet.html",
    "snippet.min.html"
  ],
  "scripts": {
    "build": "rollup --config rollup.config.js && yarn webpack",
    "lint": "eslint --ext=.ts --cache --cache-location=tmp/ . --max-warnings 0",
    "style": "prettier . --check",
    "release": "standard-version --sign",
    "upload": "gsutil -m cp -Z build/* gs://variancejs.variance.fyi/v1"
  },
  "peerDependencies": {
    "@segment/analytics-next": "^1.32.1"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "16.2.1",
    "@rollup/plugin-replace": "4.0.0",
    "@rollup/plugin-typescript": "8.3.1",
    "@segment/analytics-next": "1.34.0",
    "@typescript-eslint/eslint-plugin": "5.17.0",
    "@typescript-eslint/parser": "5.17.0",
    "clean-webpack-plugin": "4.0.0",
    "commitlint": "16.2.3",
    "eslint": "8.12.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.25.4",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-sort-keys-fix": "1.1.2",
    "html-minimizer-webpack-plugin": "3.5.0",
    "husky": "7.0.4",
    "node-gyp": "9.0.0",
    "prettier": "2.6.2",
    "rollup": "2.70.1",
    "standard-version": "9.3.2",
    "ts-loader": "9.2.8",
    "tslib": "2.3.1",
    "typescript": "4.6.3",
    "webpack": "5.71.0",
    "webpack-cli": "4.9.2"
  },
  "packageManager": "yarn@3.2.0",
  "sideEffects": false
}
